# SendGrid (Email)
SENDGRID_API_KEY=SG.your-sendgrid-api-key-here

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================

# Environment
FLUTTER_ENV=development
APP_ENV=development

# API Configuration
API_BASE_URL=https://your-api-server.com/api
API_TIMEOUT=30000

# App Configuration
APP_NAME=AI Mobile App
APP_VERSION=1.0.0
APP_BUILD_NUMBER=1

# Deep Linking
APP_SCHEME=aiapp
APP_HOST=app.yourcompany.com

# =============================================================================
# AI MODEL CONFIGURATION
# =============================================================================

# Model Settings
AI_MODEL_TEMPERATURE=0.7
AI_MODEL_MAX_TOKENS=2048
AI_MODEL_TOP_P=1.0

# Context Window Sizes
CONTEXT_WINDOW_SMALL=4096
CONTEXT_WINDOW_MEDIUM=16384
CONTEXT_WINDOW_LARGE=128000

# On-device Model Settings
ENABLE_ON_DEVICE_AI=true
MODEL_CACHE_SIZE_MB=100
MODEL_UPDATE_INTERVAL_HOURS=24

# =============================================================================
# FEATURE FLAGS
# =============================================================================

# AI Features
ENABLE_VOICE_AI=true
ENABLE_IMAGE_GENERATION=true
ENABLE_DOCUMENT_ANALYSIS=true
ENABLE_CODE_GENERATION=true
ENABLE_REAL_TIME_CHAT=true
ENABLE_OFFLINE_AI=true

# Platform Features
ENABLE_BIOMETRIC_AUTH=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_LOCATION_SERVICES=true
ENABLE_CAMERA_AI=true
ENABLE_BACKGROUND_PROCESSING=true

# =============================================================================
# DEVELOPMENT & DEBUGGING
# =============================================================================

# Logging
LOG_LEVEL=info
ENABLE_CONSOLE_LOGGING=true
ENABLE_FILE_LOGGING=false

# Debug Flags
DEBUG_AI_RESPONSES=false
DEBUG_NETWORK_CALLS=false
DEBUG_DATABASE_QUERIES=false

# Development Tools
ENABLE_FLUTTER_INSPECTOR=true
ENABLE_PERFORMANCE_OVERLAY=false

# =============================================================================
# PERSONAL DATA PLACEHOLDERS
# =============================================================================

# User Profile Defaults
DEFAULT_USER_NAME=AI Assistant User
DEFAULT_USER_EMAIL=user@example.com
DEFAULT_USER_AVATAR=https://via.placeholder.com/150

# Company Information
COMPANY_NAME=Your AI Company
COMPANY_EMAIL=contact@youraicompany.com
COMPANY_WEBSITE=https://youraicompany.com
SUPPORT_EMAIL=support@youraicompany.com
PRIVACY_POLICY_URL=https://youraicompany.com/privacy
TERMS_OF_SERVICE_URL=https://youraicompany.com/terms

# =============================================================================
# PLATFORM-SPECIFIC CONFIGURATION
# =============================================================================

# Android
ANDROID_PACKAGE_NAME=com.yourcompany.aiapp
ANDROID_MIN_SDK_VERSION=21
ANDROID_TARGET_SDK_VERSION=34

# iOS
IOS_BUNDLE_ID=com.yourcompany.aiapp
IOS_TEAM_ID=your-apple-team-id
IOS_MIN_VERSION=12.0

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================

# Certificate Pinning
ENABLE_CERTIFICATE_PINNING=true
API_CERTIFICATE_HASH=sha256/your-certificate-hash-here

# Encryption
ENCRYPTION_KEY=your-local-encryption-key-here
ENABLE_DATABASE_ENCRYPTION=true

# Biometric Authentication
BIOMETRIC_PROMPT_TITLE=Authenticate with Biometrics
BIOMETRIC_PROMPT_SUBTITLE=Use your fingerprint or face to access the app
BIOMETRIC_PROMPT_DESCRIPTION=This app uses biometric authentication for security

# =============================================================================
# SECURITY NOTES
# =============================================================================
# 1. Never commit this file with real values to version control
# 2. Use different keys for development, staging, and production
# 3. Rotate API keys regularly
# 4. Use environment-specific configurations
# 5. Enable API key restrictions where possible
# 6. Monitor API usage and set up alerts for unusual activity
# 7. Use Flutter's secure storage for sensitive data
# 8. Enable certificate pinning in production
# 9. Use obfuscation for release builds
# 10. Implement proper key management for on-device encryption
