HOST=localhost
API_BASE_URL=http://localhost:3000/api

# Frontend URLs
WEB_APP_URL=http://localhost:3001
MOBILE_APP_SCHEME=aiapp

# CORS Configuration
CORS_ORIGIN=http://localhost:3001,http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

